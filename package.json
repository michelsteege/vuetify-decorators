{
  "name": "vuetify-decorators",
  "version": "1.0.5",
  "description": "Decorators for Vuetify",
  "main": "dist/vuetify-decorators.umd.js",
  "module": "dist/vuetify-decorators.js",
  "keywords": [
    "vue",
    "vuetify",
    "typescript",
    "confirm",
    "toast",
    "snackbar",
    "decorator"
  ],
  "author": "michelsteege",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:module && npm run build:browser",
    "build:module": "bili src/vuetify-decorators.ts --format cjs --format es",
    "build:browser": "bili src/vuetify-decorators.ts --format umd --format umd-min --module-name vuetifyDecorators",
    "test": "echo \"No test specified\""
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/node": "^12.0.8",
    "bili": "^4.10.0",
    "rollup": "^1.15.6",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-vue": "^5.1.7",
    "typescript": "^3.5.2",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.11"
  },
  "typings": "./dist/vuetify-decorators.d.ts",
  "dependencies": {},
  "peerDependencies": {
    "vue": "*",
    "vuetify": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michelsteege/vuetify-decorators.git"
  },
  "bugs": {
    "url": "https://github.com/michelsteege/vuetify-decorators/issues"
  },
  "homepage": "https://github.com/michelsteege/vuetify-decorators#readme"
}
